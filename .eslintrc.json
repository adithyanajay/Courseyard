{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "globals": {
    "process": true,
    "__dirname": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-var": "error",
    "quotes": ["error", "double"],
    "no-console": "warn",
    "indent": ["warn", 2]
  }
}
